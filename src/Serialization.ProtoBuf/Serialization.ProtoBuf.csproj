<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Authors>Kris Penner</Authors>
        <Product>Kode-Aid</Product>
        <Description>Kode-Aid shared protocol buffers serialization library.</Description>
        <Copyright>Copyright Â© Kris Penner</Copyright>
        <RootNamespace>KodeAid.Serialization.ProtoBuf</RootNamespace>
    	<PackageId>KodeAid.Serialization.ProtoBuf</PackageId>
    	<Version>0.8.1</Version>
        <AssemblyName>KodeAid.Serialization.ProtoBuf</AssemblyName>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <PackageLicenseUrl>https://raw.githubusercontent.com/KodeAid/KodeAid-Core/master/LICENSE</PackageLicenseUrl>
        <PackageIconUrl>https://avatars1.githubusercontent.com/u/39310109?s=200&amp;v=4</PackageIconUrl>
        <PackageProjectUrl>https://github.com/KodeAid</PackageProjectUrl>
        <RepositoryUrl>https://github.com/KodeAid/KodeAid-Core</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>helpers extensions utilities protobuf protocol-buffers serialization</PackageTags>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <LangVersion>latest</LangVersion>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="protobuf-net" Version="2.3.7" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\Common\Common.csproj" />
    </ItemGroup>

</Project>

<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Authors>Kris Penner</Authors>
        <Product>Kode-Aid</Product>
        <Description>Kode-Aid shared common library.</Description>
        <Copyright>Copyright Â© Kris Penner</Copyright>
        <RootNamespace>KodeAid</RootNamespace>
    	<PackageId>KodeAid.Common</PackageId>
    	<Version>0.8.8</Version>
        <AssemblyName>KodeAid.Common</AssemblyName>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <PackageLicenseUrl>https://raw.githubusercontent.com/KodeAid/KodeAid-Core/master/LICENSE</PackageLicenseUrl>
        <PackageIconUrl>https://avatars1.githubusercontent.com/u/39310109</PackageIconUrl>
        <PackageProjectUrl>https://github.com/KodeAid</PackageProjectUrl>
        <RepositoryUrl>https://github.com/KodeAid/KodeAid-Core</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>helpers extensions utilities common</PackageTags>
        <LangVersion>latest</LangVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0-beta-62909-01" />
    </ItemGroup>

</Project>

<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>KodeAid.Html</AssemblyName>
    <Authors>Kris Penner</Authors>
    <Company></Company>
    <Product>Kode-Aid</Product>
    <Description>Kode-Aid shared HTML utility library.</Description>
    <Copyright>Copyright Â© Kris Penner</Copyright>
    <LangVersion>latest</LangVersion>
    <Version>0.6.0</Version>
    <RootNamespace>KodeAid.Html</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://raw.githubusercontent.com/KodeAid/KodeAid-Core/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/KodeAid</PackageProjectUrl>
    <PackageIconUrl>https://avatars1.githubusercontent.com/u/39310109?s=200&amp;v=4</PackageIconUrl>
    <RepositoryUrl>https://github.com/KodeAid/KodeAid-Core</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>helpers extensions utilities html</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack" Version="1.7.0" />
    <PackageReference Include="KodeAid.Common" Version="0.6.0" />
  </ItemGroup>

</Project>
